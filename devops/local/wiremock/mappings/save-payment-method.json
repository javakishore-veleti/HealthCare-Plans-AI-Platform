{
  "request": {
    "method": "POST",
    "urlPath": "/api/v1/payments/methods"
  },
  "response": {
    "status": 201,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "id": "pm_{{randomValue length=16 type='ALPHANUMERIC'}}",
      "type": "CREDIT_CARD",
      "brand": "Visa",
      "last4": "4242",
      "expiryMonth": 12,
      "expiryYear": 2028,
      "isDefault": false,
      "createdAt": "{{now}}"
    },
    "transformers": ["response-template"]
  }
}
